{
  "name": "winston-pretty",
  "version": "0.1.0",
  "description": "Prettifier for winston log lines",
  "keywords": [
    "winston",
    "log",
    "pretty",
    "format",
    "colorize",
    "console"
  ],
  "license": "ISC",
  "author": "Vlad-Gabriel Zahiu",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "build:watch": "tsc -p tsconfig.build.json --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run typecheck && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "rimraf": "^6.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
